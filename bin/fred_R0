#!/usr/bin/perl
use warnings;
use strict;
my ($key, $delay) = @ARGV;
die "usage: fred_R0 key delay\n" if (not $key) or (not $delay);
my $day = $delay+1;
my $result = `fred_plot --data -k $key -v RR | head -$day | tail -1`;
chomp $result;
my ($x, $r0, $std) = split " ", $result;
print "$r0 $std\n";
exit;







